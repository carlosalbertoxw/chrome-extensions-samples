{
  "manifest_version": 3,
  "name": "Popup",
  "version": "1.0.0",
  "description": "Displays a popup by clicking the extension icon.",
  "action": {
    "default_title": "Settings",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}